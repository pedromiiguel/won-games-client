{
  "name": "react-avancado-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema.graphql",
    "test:ci": "jest --runInBand",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "husky install",
    "prepare": "husky install",
    "codegen": "graphql-codegen --config codegen.yml",
    "schema:convert": "node convertToNonNull.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@graphql-codegen/near-operation-file-preset": "^2.2.13",
    "@stripe/react-stripe-js": "^1.8.1",
    "@stripe/stripe-js": "^1.31.0",
    "@styled-icons/boxicons-regular": "^10.38.0",
    "@styled-icons/boxicons-solid": "^10.38.0",
    "@styled-icons/fa-brands": "^10.38.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "@types/lodash.xor": "^4.5.7",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "graphql": "^16.3.0",
    "joi": "^17.6.0",
    "lodash.xor": "^4.5.0",
    "next": "12.0.10",
    "next-auth": "^4.3.4",
    "next-pwa": "^5.3.1",
    "next-seo": "^5.4.0",
    "nextjs-progressbar": "^0.0.14",
    "polished": "^4.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "5.3.3",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-urql": "^3.5.7",
    "@jackfranklin/test-data-bot": "1.3.0",
    "@storybook/addon-essentials": "6.4.17",
    "@storybook/builder-webpack5": "^6.4.17",
    "@storybook/manager-webpack5": "^6.4.17",
    "@storybook/react": "6.4.17",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.27",
    "@types/react-slick": "^0.23.8",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "apollo": "^2.33.10",
    "cypress": "^10.2.0",
    "cypress-plugin-stripe-elements": "^1.0.2",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.4.7",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^12.3.3",
    "msw": "^0.41.0",
    "plop": "^3.0.5",
    "prettier": "^2.4.1",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-next-router": "^3.0.8",
    "typescript": "^4.5.5",
    "urql": "^2.2.0",
    "webpack": "5.68.0"
  }
}
